generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Customer {
  id     String   @id @map("_id")
  name   String
  tables Int
  email  String
  slot   String
  date   DateTime
}

model DateTimeSlot {
  id   String   @id @map("_id")
  date DateTime
  slot String[]
}